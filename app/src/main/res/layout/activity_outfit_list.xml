<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#18CEAB">

    <ImageButton
        android:id="@+id/button_outfit_list_home_button"
        android:layout_width="106dp"
        android:layout_height="88dp"
        android:layout_marginBottom="24dp"
        android:backgroundTint="#00FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/button_outfit_ia_button"
        app:layout_constraintHorizontal_bias="0.54"
        app:layout_constraintStart_toEndOf="@+id/button_outfit_clothes_button"
        app:layout_constraintTop_toBottomOf="@+id/editTextmarcaprenda"
        app:layout_constraintVertical_bias="0.47"
        app:srcCompat="@drawable/home" />

    <ImageButton
        android:id="@+id/button_outfits_list_add"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:backgroundTint="#00FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.907"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.038"
        app:srcCompat="@drawable/signo_de_mas__1_" />

    <ImageButton
        android:id="@+id/button_outfit_clothes_button"
        android:layout_width="101dp"
        android:layout_height="110dp"
        android:backgroundTint="#000B0B0B"
        android:scaleType="centerInside"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/scroll_outfits_list_scroll"
        app:layout_constraintHorizontal_bias="0.099"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.98"
        app:srcCompat="@mipmap/tus_prendas_home_foreground" />

    <ImageButton
        android:id="@+id/button_outfit_ia_button"
        android:layout_width="121dp"
        android:layout_height="104dp"
        android:backgroundTint="#00FFFFFF"
        android:scaleType="fitStart"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/scroll_outfits_list_scroll"
        app:layout_constraintHorizontal_bias="0.938"
        app:layout_constraintStart_toStartOf="@+id/scroll_outfits_list_scroll"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.98"
        app:srcCompat="@mipmap/ia_home_foreground" />

    <TextView
        android:id="@+id/text_outfit_list_title"
        android:layout_width="379dp"
        android:layout_height="49dp"
        android:fontFamily="sans-serif-black"
        android:text="Tus outfits"
        android:textColor="#000000"
        android:textSize="34sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.098" />

    <ScrollView
        android:id="@+id/scroll_outfits_list_scroll"
        android:layout_width="414dp"
        android:layout_height="600dp"
        app:layout_constraintBottom_toTopOf="@+id/button_outfit_clothes_button"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/text_outfit_list_title"
        app:layout_constraintVertical_bias="0.482">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical" />
    </ScrollView>

</androidx.constraintlayout.widget.ConstraintLayout>